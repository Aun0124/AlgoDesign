import urllib.request
import json

endpoint = 'https://maps.googleapis.com/maps/api/directions/json?'
api_key='AIzaSyB-T4FGL-A6zZ_p6lbNgD4eHtjnV9b44a0'

origin = input('Where are you?: ').replace(' ','+')
dest = input('Where do you want to go?: ').replace(' ','+')

nav_request = 'origin={}&destination={}&mode=transit&key={}'.format(origin,dest,api_key)

request=endpoint + nav_request
response=urllib.request.urlopen(request).read()


directions=json.loads(response)
print(directions).replace

print(directions.keys)
